<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
    </PropertyGroup>
  
    <PropertyGroup>
      <TargetFramework>netcoreapp2.2</TargetFramework>
      <AssemblyName>Bayantu.Extensions.Office</AssemblyName>
      <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
      <Product>Bayantu.Extensions.Office</Product>
      <Description>八彦图Office基础库</Description>
      <PackageId>Bayantu.Extensions.Office</PackageId>
      <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
      <PackageReleaseNotes>Excel导入导出，以及与前端控件配套的解决方案服务，Word生成</PackageReleaseNotes>
      <Version>1.0.1.2</Version>
      <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
      <Company>All</Company>
      <Authors>gongyingtao</Authors>
    </PropertyGroup>
  
    <ItemGroup>
      <PackageReference Include="Autofac" Version="4.9.1" />
      <PackageReference Include="Bayantu.Extensions.Persistence" Version="1.3.1.6" />
      <PackageReference Include="Bayantu.Extensions.Session" Version="1.0.0.7" />
      <PackageReference Include="DotNetCore.NPOI" Version="1.2.1" />
      <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Solutions\Factories\" />
    </ItemGroup>

</Project>
